{
  "manifest_version": 2,
  "name": "Working Time",
  "version": "1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "css": [
        "index.css"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "index.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ]
}