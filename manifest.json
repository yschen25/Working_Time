{
  "manifest_version": 2,
  "name": "Working Time",
  "version": "1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "jquery-3.5.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}